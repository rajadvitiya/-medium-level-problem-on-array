class Solution {
    public int[] twoSum(int[] nums, int target) {
        int[] ans=new int[2];
        for(int k=0;k<nums.length-1;k++){
            for(int j=nums.length-1;j>k;j--){
                if(nums[k]+nums[j]==target){
                    ans[0]=k;
                    ans[1]=j;
                    return ans;
                }
                else{
                    continue;
                }
            }
        }
        return ans;
        
    }
}
